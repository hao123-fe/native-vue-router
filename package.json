{
  "name": "native-vue-router",
  "version": "1.0.0",
  "description": "Custom router for Vue.js 2",
  "author": "caoyu(261179233@qq.com)",
  "license": "MIT",
  "main": "dist/native-vue-router.common.js",
  "module": "dist/native-vue-router.esm.js",
  "unpkg": "dist/native-vue-router.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hao123-fe/native-vue-router"
  },
  "files": [
    "dist/*.js"
  ],
  "keywords": [
    "vue",
    "router",
    "routing"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint src examples",
    "test": "npm run lint && flow check && npm run test:unit && npm run test:e2e && npm run test:types",
    "test:unit": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
    "test:e2e": "node test/e2e/runner.js",
    "test:types": "tsc -p types/test",
    "docs": "cd docs && gitbook install && gitbook serve",
    "docs:deploy": "bash ./build/update-docs.sh",
    "release": "bash build/release.sh"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow-vue": "^1.0.0",
    "buble": "^0.15.2",
    "chromedriver": "^2.21.2",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.0.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-vue-libs": "^1.2.0",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "flow-bin": "^0.48.0",
    "gitbook-plugin-edit-link": "^2.0.2",
    "gitbook-plugin-github": "^3.0.0",
    "jasmine": "2.6.0",
    "nightwatch": "^0.9.5",
    "nightwatch-helpers": "^1.0.0",
    "path-to-regexp": "^1.5.3",
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-vue": "^2.4.2",
    "rollup-watch": "^4.0.0",
    "sass-loader": "^6.0.6",
    "selenium-server": "^2.53.1",
    "typescript": "^2.0.3",
    "uglify-js": "^3.0.17",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.2",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
